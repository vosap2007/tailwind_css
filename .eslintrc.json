{
  "extends": ["eslint:recommended", "next", "prettier"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true
      }
    ]
  },
  "plugins": ["react", "prettier"],
  "env": {
    "browser": true,
    "node": true
  },
  "ignorePatterns": ["dist", "node_modules", "*/.test.js"]
}
